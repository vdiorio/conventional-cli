{
  "name": "conventional-cli",
  "version": "1.0.0",
  "description": "A conventional commits client for padronized commit messages.",
  "main": "./lib/index.js",
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "build": "tsc -p .",
    "local": "npm i --location=global && conv"
  },
  "keywords": [
    "conventional",
    "commit",
    "vdiorio"
  ],
  "bin": {
    "conv": "./lib/index.js"
  },
  "author": "vdiorio",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^18.6.2",
    "nodemon": "^2.0.19",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
