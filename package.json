{
  "name": "gitconv",
  "version": "1.0.2",
  "description": "A conventional commits client for padronized commit messages.",
  "main": "./lib/index.js",
  "bin": {
    "gitconv": "./lib/index.js"
  },
  "scripts": {
    "start": "node --loader ts-node/esm src/index.ts",
    "build": "tsc -p .",
    "local": "npm run build && npm i . --location=global",
    "lint": "eslint --fix --ext .ts,.tsx .",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install"
  },
  "keywords": [
    "conventional",
    "commit",
    "vdiorio",
    "cli"
  ],
  "author": "vdiorio",
  "homepage": "https://github.com/vdiorio/gitconv-cli",
  "license": "ISC",
  "devDependencies": {
    "@types/inquirer": "^8.2.1",
    "@types/inquirer-autocomplete-prompt": "^1.3.4",
    "@types/node": "^18.6.2",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.26.0",
    "nodemon": "^2.0.19",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "colors": "^1.4.0",
    "execa": "5.1.1",
    "fuse.js": "6.6.2",
    "inquirer": "^8.2.4",
    "inquirer-autocomplete-prompt": "^2.0.0"
  }
}
